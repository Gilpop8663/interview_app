{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": ["app/*"],
      "@utils/*": ["src/utils/*"],
      "@models/*": ["src/types/*"],
      "@contexts/*": ["src/contexts/*"],
      "@fetcher/*": ["src/fetcher/*"],
      "@gql/*": ["src/gql/*"],
      "@hooks/*": ["src/hooks/*"],
      "@constants/*": ["src/constants/*"],
      "@components/*": ["src/components/*"],
      "@src/*": ["src/*"]
    }
  }
}
